generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  password    String
  credenciais credenciais[]
}

model credenciais {
  id      Int    @id @default(autoincrement())
  userId  Int
  titulo  String
  nome    String
  rotulo  String
  url     String
  usuario String
  senha   String
  users   users  @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
